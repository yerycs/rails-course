<h1>Enrollments new</h1>
<p>Find me in app/views/enrollments/new.html.erb</p>

<%= form_for @enrollment do |f| %>
  <%= f.label :course_id %>
  <%= f.collection_select :course_id, Course.all, :id, :name %>
  <%= f.label :student_id %>
  <%= f.collection_select :student_id, Student.all, :id, :name %>
  <%= f.submit %>
<% end %>
